{
  "name": "frontend-interview",
  "version": "1.0.0",
  "description": "Challenge for B2W",
  "scripts": {
    "build": "NODE_ENV=development brunch build",
    "build:prod": "NODE_ENV=production brunch build --production",
    "start": "NODE_ENV=development brunch watch --server"
  },
  "author": "Vinícius Lagêdo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viniciuslagedo/frontend-interview/issues"
  },
  "homepage": "https://github.com/viniciuslagedo/frontend-interview#readme",
  "dependencies": {
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^6.0.6",
    "babel-core": "^6.18.2",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bootstrap-styl": "^5.0.8",
    "brunch": "^2.8.2",
    "javascript-brunch": "^2.0.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "sass-brunch": "^2.6.3",
    "stylus-brunch": "^2.7.2"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^2.2.0",
    "eslint": "^3.8.1",
    "eslint-brunch": "^3.11.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "fb-flo-brunch": "^1.7.22"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "globals": {
      "window": true,
      "document": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "semi": 0,
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  }
}
